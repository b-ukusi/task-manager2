span(style="font-family: verdana, geneva, sans-serif;")
  doctype html
  html(lang="en")
    head
      meta(charset="UTF-8")
      title Client Dashboard
      link(rel="stylesheet",  type='text/css', href="/stylesheets/clientnotes.css")
      //- Font Awesome Cdn Link -->
      link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css")
    body
      .container
        nav
          div 
            h1 
              img(src="/images/LOGO33.jpg", alt="Admin")
              Admin
            ul
              li
                a(href="/client/", class="logo")
                  h2 DashBoard
              
              li
                a(href="/client/notes")
                  i.fas.fa-edit
                  span.nav-item Notes
              li
                a(href="/client/projects")
                  i.fas.fa-cogs
                  span.nav-item Projects  
              //- li
                a(href="/client/account")
                  i.fas.fa-home
                  span.nav-item Account Details 
                  
          ul
              a(href="/", class="logout")
                i.fas.fa-sign-out-alt
                span.nav-item Log out
        section.main 
          h1 Notes  
            .card22
              each note in notes
                  .card
                    .card-body
                        p= `Note ID : ${note.noteid}`
                        p= `Task ID: ${note.taskid}`
                        p= `Note Name : ${note.notename}`
                        p= `Description : ${note.description}`

                       